                        android:layout_marginBottom="@dimen/_10sdp"
                        android:id="@+id/date_1901_layout"/>

                    <TextView
                        android:id="@+id/date_1905"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/date_1905"
                        android:layout_marginBottom="@dimen/_10sdp"/>


